<%@ page contentType="text/html;charset=UTF-8" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<div class="row fu-margin-none fu-margin-bottom">
	<div class="col-md-12 fu-border-bottom">
		<h1><small>数据搜索</small></h1>
	</div>
</div>
<div class="row fu-margin-left fu-margin-right fu-margin-bottom fu-background fu-padding">
	<form role="form" id="Form_kind_search" onsubmit="return fSearch()">
		<div class="form-group col-md-4">
			<label for="account">类别名称</label>
			<input type="text" class="form-control" name="name" id="name" class="form-control" placeholder="类别名称" aria-describedby="basic-addon2">
		</div>
		<div class="form-group col-md-12 text-right">
			<button type="submit" class="btn btn-primary btn-sm">
				<i class="glyphicon glyphicon-search"></i>搜索
			</button>
			<button type="reset" class="btn btn-danger btn-sm">
				<i class="glyphicon glyphicon-erase"></i>清空
			</button>
			<button type="button" id="Button_kind_add" class="btn btn-success btn-sm" onclick="fChoiceSelect()" data-toggle="modal" data-target="#Modal_kind_add" >
				<i class="glyphicon glyphicon-plus"></i>添加
			</button>
		</div>
	</form>
</div>