<%@ page contentType="text/html;charset=UTF-8"%>
<!-- 查询 start-->
<form id="appointmentsearchform" class="form">
	<div class="form-group col-md-3">
		<label class="control-label">开始时间</label>
		<input type="text" class="form-control tRequir" name="startTime" readonly="readonly">
	</div>
	<div class="form-group col-md-3">
		<label class="control-label">结束时间</label>
		<input type="text" class="form-control tRequir" name="endTime" readonly="readonly">
	</div>
	<div class="form-group col-md-3">
		<label class="control-label">标题</label>
		<input class="form-control" size="16" name="subject" type="text">
	</div>
	<div class="form-group col-md-3">
		<label class="control-label">内容</label>
		<input class="form-control" size="16" name="content" type="text">
	</div>
	<input type="hidden" id="operationId" name="operation"
		value="partrefresh">
	<div class="form-group col-md-12">
		<button id="appointmentsearchbtn" class="btn btn-success btn-sm"
			type="button">
			<i class="glyphicon glyphicon-search"></i>
		</button>
	</div>
</form>
<!-- 查询 end-->