//二级菜单获取
function getMenu(layer,menuId){
	if(menuId == "9c8a0b05815c400cafd62594b175e35b"){
		systemSetting();
	}else if(menuId == "52d589059b82456594e8be538d880d33"){
		window.location.href = jypath +'/backstage/index3?layer='+layer+'&menuId='+menuId+'&ref=y';
	}else {
		window.location.href = jypath +'/backstage/index2?layer='+layer+'&menuId='+menuId+'&ref=y';
	}
}
//系统设置
function systemSetting(){
	window.location.href = jypath +'/backstage/systemSetting';
}

