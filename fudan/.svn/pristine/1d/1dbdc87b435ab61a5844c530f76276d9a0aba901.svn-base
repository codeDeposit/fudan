<script type="text/javascript">
   	//加载数据..
   	$(document).ready(function() {
   		fuLoading();
		// 初始化日期组件
	    $('.form_date').datetimepicker({
	        language: 'zh-CN',
	        weekStart: 1,
	        todayBtn: 1,
	        autoclose: 1,
	        todayHighlight: 1,
	        startView: 2,
	        minView: 2,
	        forceParse: 0,
	    });
		//初始化菜单
	    $('.secondT').on('click', function(event) {
            var theLev=$(this).parent().find('.secondLevel');
            if(theLev.css('display')=='block'){
                theLev.slideUp('fast');
                $(this).find('.glyphicon').removeClass('glyphicon-minus').addClass('glyphicon-plus');
            }else{
                $('.secondLevel').slideUp('fast');
                theLev.slideToggle('fast');
                $('.ftof').removeClass('glyphicon-minus').addClass('glyphicon-plus');
                $(this).find('.glyphicon').removeClass('glyphicon-plus').addClass('glyphicon-minus');
            }
        });
        $('.thirdT').on('click', function(event) {
            var theLev=$(this).parent().find('.thirdLevel');
            if(theLev.css('display')=='block'){
                theLev.slideUp('fast');
                 $(this).find('.glyphicon').removeClass('glyphicon-minus').addClass('glyphicon-plus');
            }else{
                $('.thirdLevel').slideUp('fast');
                theLev.slideToggle('fast');
                $('.tof').removeClass('glyphicon-minus').addClass('glyphicon-plus');
                $(this).find('.glyphicon').removeClass('glyphicon-plus').addClass('glyphicon-minus');
            }
        });
        $('.aHover').on('click',function(event) {
            event.preventDefault();
            $('.aHover').removeClass('active');
            $(this).addClass('active');
        });
        //加载数据..办公用品类别，这个是填充到搜索框的办公用品类别
   		getOfficeGoodsType();
   		//加载数据...办公用品类别，这个是填充到选择办公用品的弹窗左侧菜单栏
   		getOfficeGoodsTypeToLeftMenu();
   		//加载数据...默认加载所有商品,填充到办公用品弹窗右侧表格
   		getAllGoods();
   		//初始化数据..加载所有部门
		getAllDep();
		//初始化数据..加载所有角色
		getAllRole();
   	});
   	function getOfficeGoodsType() {
   		var url = "/oa/officegoodstype/listall";
   		$.ajax({
   			type: "get",
   			url : jypath + url,
   			success: function(data) {
   				var objs = eval("("+data+")");
   				var html = "<option value='0'>---请选择---</option>";
   				for(var index in objs) {
   					var obj = objs[index];
   					html += "<option value='"+obj.typeId+"'>"+obj.typeName+"</option>";
   				}
   				$("#depretypeselect").html(html);
   			}
   		});
   	}
   	//加载所有商品类别
   	function getOfficeGoodsTypeToLeftMenu() {
   		var url = "/oa/officegoodstype/listhtml";
   		$.ajax({
   			type: "get",
   			url : jypath + url,
   			success: function(data) {
   				var html = eval("("+data+")");
   				$("#goodstype").html(html);
   			}
   		});
   	}
   	//加载商品类别下的所有商品
   	function getGoodsByType(goodstypeId) {
   		var url = "/oa/officegoods/listbytype";
   		$.ajax({
   			type: "get",
   			url : jypath + url,
   			data: "typeId="+goodstypeId,
   			success: function(data) {
   				var html = eval("("+data+")");
   				$("#goodstable").html(html);
   			}
   		});
   	}
   	//加载所有商品
   	function getAllGoods() {
   		var url = "/oa/officegoods/listall";
   		$.ajax({
   			type: "get",
   			url : jypath+url,
   			success: function(data) {
   				var html = eval("("+data+")");
   				$("#goodstable").html(html);
   			}
   		});
   	}
   	//加载所有部门
  	function getAllDep() {
    	var url = "/oa/org/listorg";
    	$.ajax({
    		type: "get",
    		url : jypath + url,
    		success: function(data) {
    			var obj = eval("("+data+")");
    			$("#orgs").html(obj);
    		}
    	});
    }
    //加载所有角色
  	function getAllRole() {
    	var url = "/oa/role/listhtml";
    	$.ajax({
    		type: "get",
    		url : jypath + url,
    		success: function(data) {
    			var obj = eval("("+data+")");
    			$("#roles").html(obj);
    			fuCloseLayer();
    		}
    	});
    }
    //根据图书类别显示图书
   	function getBookByType(iBookTypeId) {
   		var url = "/oa/book/listhtml";
   		
   		$.ajax({
   			type: "get",
   			url : jypath+url,
   			data: "typeId="+iBookTypeId,
   			success: function(data) {
   				//打印数据
   				$("#booktable").html(data);
   				$("#booktableedit").html(data);
   			}
   		});
   	}
  	//判断是否选中
    $('.aYes').on('click',function(){
    	var oarrayName = [];
    	var iarrayId = [];
	    $('#goodstable').find('input[type="checkbox"]').each(function(index, el) {
	        if($(this).is(':checked')){
	        	//var abc=$(this).parent().parent().children('td').eq(2).html();
	        	var obj = $(this).parent().parent().children('td').eq(2);
	        	iGoodsId = obj.attr("id");
	        	var objName=obj.html();
	        	oarrayName.push(objName);
	        	iarrayId.push(iGoodsId);
	        	$("#goodsnames").val(oarrayName);
	        	$("#goodsIds").val(iarrayId);
	        	$("#surebtn").attr("data-dismiss", "modal");
	        }
	    });
    });
    //判断是否选中
    $('.bYes').on('click',function(){
    	var oarrayName = [];
    	var iarrayId = [];
	    $('#employeetable .tBody').find('input[type="checkbox"]').each(function(index, el) {
	        if($(this).is(':checked')){
	        	//var abc=$(this).parent().parent().children('td').eq(2).html();
	        	var obj = $(this).parent().parent().children('td').eq(2);
	        	iGoodsId = obj.attr("id");
	        	var objName=obj.html();
	        	oarrayName.push(objName);
	        	iarrayId.push(iGoodsId);
	        	$("#empNames").val(oarrayName);
	        	$("#empIds").val(iarrayId);
	        	$("#bgrsurebtn").attr("data-dismiss", "modal");
	        }
	    });
    });
    //获取某个部门下的所有员工
  	function getEmpByOrg(id) {
    	var url = "/oa/listEm";
    	id= id+"";
    	$.ajax({
    		type: "get",
    		url : jypath + url,
    		data: "orgId="+id,
    		success: function(data) {
    			$("#employeetable").html(data);
    		}
    	});
    }
  	//获取某角色下的所有员工
  	function getEmpByRole(id) {
    	var url = "/oa/listEm";
    	id += "";
    	$.ajax({
    		type: "get",
    		url : jypath + url,
    		data: "roleId="+id,
    		success: function(data) {
    			$("#employeetable").html(data);
    		}
    	});
    }
   	// 添加类别
   	$("#booktypeaddbtn").click(function() {
   		$.ajax({
			type: "post",
   			url:jypath + "/oa/goodsapply/add",
   			data: $("#booktypeaddform").serialize(),
   			success : function(html) {
   				$("#booktypeaddbtn").attr("data-dismiss", "modal");
   				fuShowSuccess();
   				// 刷新页面
   				refresh();
   			}
   		});
   	});
   	// 查看详细信息或修改
   	function openForm(id, operation) {
   		fuLoading();
		var url = "/oa/goodsapply/getdetails";
		$.ajax({
			type: "get",
   			url : jypath+url,
   			data: "applyId="+id+"&operation="+operation,
   			success: function(data){
   				fuCloseLayer();
		    	$("#officegoodstypeInfoOrUpdate").html(data);
		    }
   		});
   	}
	// 查询
	$("#officegoodstypesearchbtn").click(function() {
		fuLoading();
		var id = "officegoodstypetable";
		var url = jypath + "/oa/goodsapply/list";
		var type = "get";
		var data = $("#officegoodstypesearchform").serialize();
		partRefresh(id, url, type, data);
	});
	// pageNos: 第几页
	function pageturn(pageNos) {
		fuLoading();
		if(pageNos == 0) {
			pageNos = $("#zhuandao").val();
		}
		var id = "officegoodstypetable";
		var url= jypath+"/oa/goodsapply/list";
		var type = "get";
		var data = $("#officegoodstypesearchform").serialize()+"&pageNos="+pageNos;
		pageturning(id, url, type, data);
	}
	//清除内容
	function removeData(id) {
    	$("#"+id).val("");
    }
    $("#goodsbtn").on("click", function() {
    	removeData("goodsnames");
    	removeData("goodsIds");
    });
    $("#empbtn").on("click", function() {
    	removeData("empNames");
    	removeData("empIds");
    });
    function fuSure(param){
		fuLoading();
		fuClose();
		setTimeout('fDelete(\''+param+'\')',200);
	}
	// 删除
	function fDelete(typeId) {
		var url = "/oa/goodsapply/multidel";
		$.ajax({
			type: "get",
			url: jypath+url+"?applyId="+typeId,
			success : function(html) {
				fuShowSuccess();
   				refresh();
   			}
		});
	}
