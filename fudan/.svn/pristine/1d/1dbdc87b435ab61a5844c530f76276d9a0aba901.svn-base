		<div id="officegoodstypetable">
		<table class="table table-striped table-bordered table-hover dataTables-example">
	        <thead>
	           <tr>
	               <th>序号</th>
	               <th>商品名称</th>
	               <th>申请号</th>
	               <th>申请人</th>
	               <th>申请数量</th>
	               <th>申请日期</th>
	               <th>备注</th>
	               <th>审批状态</th>
	               <th>操作</th>
	           </tr>
	       </thead>
	       <tbody>
	       		<c:choose>
				<c:when test="${goodsApplys != null and fn:length(goodsApplys) != 0}">
	       		<c:forEach var="goodsApply" items="${goodsApplys}" varStatus="index">
	           	<tr class="gradX">
	              	<td>${index.count}</td>
	              	<td>${goodsApply.officeGoods.goodsName}</td>
	              	<td><a href="#home" onclick="openForm('${goodsApply.applyId}', 'info')" data-toggle="modal" data-target="#officegoodstypeInfoOrUpdate">${goodsApply.applyNo}</a></td>
	              	<td>${goodsApply.proposer}</td>
	              	<td>${goodsApply.useCounts}</td>
	              	<td>${goodsApply.applyDate1}</td>
	              	<td>${goodsApply.notes}</td>
	              	<td>${goodsApply.approvalStatus}</td>
