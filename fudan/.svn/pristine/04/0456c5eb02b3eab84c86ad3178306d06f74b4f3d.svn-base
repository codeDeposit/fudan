<%@ page contentType="text/html;charset=UTF-8" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<div class="row fu-margin-none fu-margin-bottom">
	<div class="col-md-12 fu-border-bottom">
  		<h1><small>数据搜索</small></h1>
	</div>
</div>
<div class="row fu-margin-left fu-margin-right fu-margin-bottom fu-background fu-padding">
	<form role="form" id="supplierSearchForm" onsubmit="return fSearch()">
		<div class="form-group col-md-2">
			<label for="account">经营范围</label></span>
			<select class="form-control input-sm" id="business_scope" name="business_scope">
				<option value="">--请选择--</option>
			</select>
		</div>
		<div class="form-group col-md-2">
			<label for="account">状态</label></span>
			<select class="form-control input-sm" id="state" name="state">
				<option value="">--请选择--</option>
				<option value="1">已注册</option>
				<option value="2">初审中</option>
				<option value="3">初审通过</option>
				<option value="4">初审未通过</option>
				<option value="5">已冻结</option>
				<option value="6">完成入库</option>
			</select>
		</div>
		<div class="form-group col-md-2">
			<label for="account">注册地址（省）</label></span>
			<select class="form-control input-sm" id="company_address1" name="company_address1" onchange="getCity()">
				<option value="">-- 请选择 --</option>
			</select>
		</div>
		<div class="form-group col-md-2">
			<label for="account">注册地址（市）</label></span>
			<select class="form-control input-sm" id="company_address2" name="company_address2">
				<option value="">-- 请选择 --</option>
			</select>
		</div>
		<div class="form-group col-md-2">
			<label for="account">注册资金（大于等于）</label></span>
			<div class="input-group">
				<input type="text" class="form-control input-sm" id="bankrollMi" name="bankrollMi" maxlength="20" placeholder="大于"  >
				<span class="input-group-addon">万元</span>
			</div>
		</div>
		<div class="form-group col-md-2">
			<label for="account">注册资金（小于等于）</label></span>
			<div class="input-group">
				<input type="text" class="form-control input-sm" id="bankrollMa" name="bankrollMa" maxlength="20" placeholder="小于"  >
				<span class="input-group-addon">万元</span>
			</div>
		</div>
		<div class="form-group col-md-4">
			<label for="account">成立时间（大于等于）</label></span>
			<div class="input-group">
				<div class="input-group date form_date" data-date-format="yyyy-MM-dd">
		            <input class="form-control" id="found_time_min" name="found_time_min" type="text" readonly >
		            <span class="input-group-addon"><span class="glyphicon glyphicon-remove"></span></span>
		            <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
		        </div>
	       </div>
		</div>
		<div class="form-group col-md-4">
			<label for="account">成立时间（小于等于）</label></span>
			<div class="input-group">
				<div class="input-group date form_date" data-date-format="yyyy-MM-dd">
		            <input class="form-control" id="found_time_max" name="found_time_max" type="text" readonly >
		            <span class="input-group-addon"><span class="glyphicon glyphicon-remove"></span></span>
		            <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
		        </div>
	       </div>
		</div>
		<div class="form-group col-md-12">
			<button type="submit" class="btn btn-primary btn-sm"><i class="glyphicon glyphicon-search"></i>搜索</button>
			<button type="reset" class="btn btn-danger btn-sm"><i class="glyphicon glyphicon-erase"></i>清空</button>
			<button type="button" class="btn btn-info btn-sm" onclick="fExcel(1)"><i class="glyphicon glyphicon-download-alt"></i>导出</button>
		</div>
	</form>
</div>