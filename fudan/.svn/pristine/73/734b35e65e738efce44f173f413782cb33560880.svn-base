<%@ page contentType="text/html;charset=UTF-8"%>
<!-- 查询 start-->
<form id="phonebooksearchform" class="SearchForm">
	<div class="form-group col-md-4">
		<label for="phoneGroupSelect" class="control-label">联系人分组</label>
		<select id="phoneGroupSelect" class="form-control" name="groupId">
			<option selected value="">--请选择--</option>
		</select>
	</div>
	<div class="form-group col-md-4">
		<label for="phoneGroupSelect" class="control-label">称谓</label>
		<select id="phoneGroupSelect" class="form-control" name="title">
			<option selected value="">--请选择--</option>
			<option value="先生">先生</option>
			<option value="女士">女士</option>
		</select>
	</div>
	<div class="form-group col-md-4 col-sm-4">
		<label for="fullnamesearch" class="control-label">姓名</label>
		<input type="text" id="fullnamesearch" class="form-control" name="fullname">
	</div>
	<input type="hidden" id="operationId" name="operation"
		value="partrefresh">
	<div class="form-group col-md-12">
		<button id="phonebooksearchbtn" class="btn btn-success btn-sm" type="button">
			<i class="glyphicon glyphicon-search"></i>
		</button>
	</div>
</form>
<!-- 查询 end-->