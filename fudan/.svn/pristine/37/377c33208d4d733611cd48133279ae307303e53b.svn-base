<%@ page contentType="text/html;charset=UTF-8"%>
<!-- 查询 start-->
<form id="sharesearchform" class="form-horizontal" role="form"
	action="${jypath}/oa/phonebook/sharelist" method="post">
	<div class="form-group col-md-4">
		<label for="fullnamesearch" class="control-label col-md-3 text-center">姓名</label>
		<div class="col-md-9">
			<input type="text" id="fullnamesearch" class="form-control" name="fullname">
		</div>
	</div>
	<div class="form-group col-md-4">
		<label for="sharenamesearch" class="control-label col-md-3 text-center">共享人</label>
		<div class="col-md-9">
			<input type="text" id="sharenamesearch" class="form-control" name="shareName">
		</div>
	</div>
	<input type="hidden" id="operationId" name="operation" value="partrefresh">
	<div class="form-group col-md-1">
	<button id="phonebooksharesearchbtn" class="btn btn-success btn-sm" type="button">
		<i class="glyphicon glyphicon-search"></i>
	</button>
	</div>
</form>
<!-- 查询 end-->