<%@ page contentType="text/html;charset=UTF-8"%>
<!-- 查询 start-->
<form id="newssearchform" class="form">
	<div class="form-group col-md-5">
		<label class="control-label">是否为个人计划</label> <select
			id="newsTypeSelect" class="form-control"
			name="isPersonal">
			<option selected value="">--请选择--</option>
			<option value="1">是</option>
			<option value="0">否</option>
		</select>
	</div>
	<div class="form-group col-md-5">
		<label class="control-label">计划名称</label>
		<input type="text" class="form-control" name="planName">
	</div>
	<input type="hidden" id="operationId" name="operation"
		value="partrefresh">
	<div class="form-group col-md-12">
		<button id="newssearchbtn" class="btn btn-success btn-sm" type="button">
			<i class="glyphicon glyphicon-search"></i>
		</button>
	</div>
</form>
<!-- 查询 end-->