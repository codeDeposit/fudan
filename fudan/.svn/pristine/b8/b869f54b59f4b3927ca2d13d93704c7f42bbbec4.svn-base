<%@ page contentType="text/html;charset=UTF-8"%>
<!-- 查询 start-->
<form id="phoneGroupSearchform" class="SearchForm">
	<div class="form-group col-md-2 col-sm-2">
		<input type="text" placeholder="分组名称" class="form-control forinBlo"
			style="width:100%;" name="groupName">
	</div>
	<div class="form-group col-md-3 col-sm-3">
		<label class="control-label col-sm-4 labelPad">是否共享</label> <select
			id="newsTypeSelect" class="form-control col-sm-8 forinBlo"
			name="isShared">
			<option selected value="">--请选择--</option>
			<option value="0">否</option>
			<option value="1">是</option>
		</select>
	</div>
	<input type="hidden" id="operationId" name="operation"
		value="partrefresh">
	<button id="phonegroupsearchbtn" class="btn btn-success btn-sm"
		type="button">
		<i class="glyphicon glyphicon-search"></i>
	</button>
</form>
<!-- 查询 end-->